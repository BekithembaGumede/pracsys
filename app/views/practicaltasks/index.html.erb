<p id="notice"><%= notice %></p>

<h1>Listing Practicaltasks</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Taskname</th>
      <th>Description</th>
      <th>Duedate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @practicaltasks.each do |practicaltask| %>
      <tr>
        <td><%= practicaltask.course %></td>
        <td><%= practicaltask.taskname %></td>
        <td><%= practicaltask.description %></td>
        <td><%= practicaltask.duedate %></td>
        <td><%= link_to 'Show', practicaltask %></td>
        <td><%= link_to 'Edit', edit_practicaltask_path(practicaltask) %></td>
        <td><%= link_to 'Destroy', practicaltask, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Practicaltask', new_practicaltask_path %>
